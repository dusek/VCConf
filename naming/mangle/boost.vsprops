<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Boost Name Mangling"
	>
	<Tool
		Name="VCLinkerTool"
		OutputFile="$(MyTargetDirNPrefix)$(MangledConfig)$(MyTargetExt)"
	/>
	<UserMacro
		Name="ThreadingSingle"
		Value=""
	/>
	<UserMacro
		Name="ThreadingMulti"
		Value="-mt"
	/>
	<UserMacro
		Name="DebugInfoYes"
		Value="-gd"
	/>
	<UserMacro
		Name="DebugInfoNo"
		Value=""
	/>
	<UserMacro
		Name="ToolsetVersion"
		Value="-$(VCCToolsetVersion)"
	/>
	<UserMacro
		Name="MangledConfig"
		Value="$(Threading)$(DebugInfo)$(ToolsetVersion)"
	/>
	<UserMacro
		Name="MyTargetPrefix"
		Value="$(SolutionName)"
	/>
	<UserMacro
		Name="MyTargetDirNPrefix"
		Value="$(OutDir)$(MyTargetPrefix)"
	/>
	<UserMacro
		Name="Threading"
		Value="$(ThreadingMulti)"
	/>
</VisualStudioPropertySheet>
